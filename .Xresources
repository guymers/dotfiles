Xft.dpi: 112
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

XTerm*faceName: monospace
XTerm*faceSize: 9
XTerm*selectToClipboard: true

*saveLines: 5000

URxvt*font: xft:monospace:size=9
URxvt*letterSpace: -1
URxvt*scrollBar: false
URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

URxvt*perl-ext:
URxvt*perl-ext-common: clipboard
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard
URxvt.clipboard.autocopy: true

xscreensaver.timeout: 0:10:00
xscreensaver.cycle: 0:00:00
xscreensaver.lock: true
xscreensaver.lockTimeout: 0:00:30
xscreensaver.mode: blank
xscreensaver.dpmsEnabled: true
xscreensaver.dpmsStandby: 0:10:00
xscreensaver.dpmsSuspend: 2:00:00
xscreensaver.dpmsOff: 4:00:00
xscreensaver.dpmsQuickOff: true
xscreensaver.splash: false
xscreensaver.fade: false

